Layout
 *main
   contains:res, buttons
   *res
     contains:Resources
     class:fullWidth
   *buttons
     contains:Buttons
 *store
   contains:buildings, upgrades
   *buildings
     contains:BulkDisplay, Buildings
     header:Buildings
     tooltip origin:left
     ps:show
   *upgrades
     contains:Upgrades, tag:upgrade, tag:research
     header:Upgrades
     costs:hide
     names:hide

Buttons

 *georgebutton
  on click:gain 1 george
  on click:if (have coryunlock) gain 1 cory
  on click:if (have gregunlock) gain 0.1 greg
  class:bigButton
  icon:
  no tooltip

Includes
////////////////////////////////////////////////////////////////////////////////////BUILDING UPGRADES

 *include boxup %id %name %flavor="There's nothing interesting here." %req %cost %icon="0"
  *boxup[%id]
    name:[%name]
    desc:Boxes are <b>twice</b> as efficient.<q>"[%flavor]"</q>
    req:[%req] box
    cost:[%cost] georges
    passive:multiply george yield of box by 2
    icon:i[[%icon],5]
    tags:upgrade, tier[%id]
    
 *include standup %id %name %flavor="There's nothing interesting here." %req %cost %icon="0"
  *standup[%id]
    name:[%name]
    desc:Stands are <b>twice</b> as efficient.<q>"[%flavor]"</q>
    req:[%req] stand
    cost:[%cost] georges
    passive:multiply george yield of stand by 2
    icon:i[[%icon],6]
    tags:upgrade, tier[%id]
    
 *include shopup %id %name %flavor="There's nothing interesting here." %req %cost %icon="0"
  *shopup[%id]
    name:[%name]
    desc:Shops are <b>twice</b> as efficient.<q>"[%flavor]"</q>
    req:[%req] shop
    cost:[%cost] corys
    passive:multiply george yield of shop by 2
    passive:multiply cory yield of shop by 2
    icon:i[[%icon],7]
    tags:upgrade, tier[%id]
    
 *include vendup %id %name %flavor="There's nothing interesting here." %req %cost %icon="0"
  *vendup[%id]
    name:[%name]
    desc:Vending Machines are <b>twice</b> as efficient.<q>"[%flavor]"</q>
    req:[%req] vend
    cost:[%cost] corys
    passive:multiply george yield of vend by 2
    passive:multiply cory yield of vend by 2
    icon:i[[%icon],8]
    tags:upgrade, tier[%id]
    
 *include weathup %id %name %flavor="There's nothing interesting here." %req %cost %icon="1"
  *weathup[%id]
    name:[%name]
    desc:Weather Machines are <b>twice</b> as efficient.<q>"[%flavor]"</q>
    req:[%req] weath
    cost:[%cost] gregs
    passive:multiply george yield of weath by 2
    passive:multiply cory yield of weath by 2
    passive:multiply greg yield of weath by 2
    icon:i[[%icon],9]
    tags:upgrade, tier[%id]

 *include orchup %id %name %flavor="There's nothing interesting here." %req %cost %icon="1"
  *orchup[%id]
    name:[%name]
    desc:Orchards are <b>twice</b> as efficient.<q>"[%flavor]"</q>
    req:[%req] orch
    cost:[%cost] gregs
    passive:multiply george yield of orch by 2
    passive:multiply cory yield of orch by 2
    passive:multiply greg yield of orch by 2
    passive:increase time yield of orch by 0.5
    icon:i[[%icon],10]
    tags:upgrade, tier[%id]

 *include pressup %id %name %flavor="There's nothing interesting here." %req %cost %icon="1"
  *pressup[%id]
    name:[%name]
    desc:Presses are <b>twice</b> as efficient.<q>"[%flavor]"</q>
    req:[%req] press
    cost:[%cost] gregs
    passive:multiply george yield of press by 2
    passive:multiply cory yield of press by 2
    passive:multiply greg yield of press by 2
    icon:i[[%icon],11]
    tags:upgrade, tier[%id]
    
////////////////////////////////////////////BEVERAGES

 *include georgeade %id %name %flavor="There's nothing interesting here." %req %cost %icon="0"
  *georgeade[%id]
   name:[%name]
   desc:Lemon production +1%.<q>"[%flavor]"</q>
   passive:multiply yield of georges by 1.01
   cost:[%cost] georges
   req:[%req] georges:earned
   icon:i[[%icon],2]
   tag:upgrade
   
 *include coryade %id %name %flavor="There's nothing interesting here." %req %cost %icon="0"
  *coryade[%id]
   name:[%name]
   desc:cory production +1%.<q>"[%flavor]"</q>
   passive:multiply yield of corys by 1.01
   cost:[%cost] corys
   req:[%req] corys:earned
   icon:i[[%icon],3]
   tag:upgrade
   
 *include gregade %id %name %flavor="There's nothing interesting here." %req %cost %icon="0"
  *gregade[%id]
   name:[%name]
   desc:greg production +1%.<q>"[%flavor]"</q>
   passive:multiply yield of gregs by 1.01
   cost:[%cost] greg
   req:[%req] gregs:earned
   icon:i[[%icon],4]
   tag:upgrade
   
