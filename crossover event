 [i:totcoryach %id="1" %name="Once upon a cory" %desc="Make 1 cory." %req="1" %icon="0"]
 [i:totcoryach %id="2" %name="Need a juicer?" %desc="Make 1,000 corys." %req="1000" %icon="1"]
